<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practica">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practica">
        <activity android:name=".EditEvent" />
        <activity android:name=".ListChatActivity" />
        <activity android:name=".ParticipatedEvents" />
        <activity android:name=".NewEventActivity" />
        <activity android:name=".EventActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".UserOptions" />
        <activity android:name=".PerfilOption" />
        <activity android:name=".CreatedEventsOption" />
        <activity android:name=".SavedEventsOption" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListEvents" />
        <activity
            android:name=".LogoutDialog"
            android:theme="@style/LogoutDialog" />
        <activity
            android:name=".ChangeTextDialog"
            android:theme="@style/LogoutDialog" />
        <activity
            android:name=".PuntuationDialog"
            android:theme="@style/LogoutDialog" />

    </application>

</manifest>